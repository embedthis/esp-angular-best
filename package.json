{
    "name": "esp-best",
    "description": "ESP Best Practices Angular",
    "version": "5.3.0",
    "keywords": [
        "esp"
    ],
    "dependencies": {
        "esp-angular-skeleton": "~5.3",
        "bootstrap": "~3",
        "angular-bootstrap": ">=0.7.0",
        "html5shiv": "*",
        "font-awesome": "*",
        "more": "*",
        "respond": "*"
    },
    "licenses": [ {
            "type": "GPL",
            "url": "http://www.gnu.org/licenses/gpl-2.0.html"
        }, {
            "type": "Commercial",
            "url": "https://embedthis.com/licensing/"
        }
    ],
    "export": [
        {  
            "from": "js/*.js", 
            "to": "esp-best/" 
        }, {  
            "from": "source/**", 
            "to": "${TOP}", 
            "overwrite": false
        }
    ],
    "render": {
        "js": "js/*.js"
    },
    "blend": [
        "esp.json"
    ]
}
