{
    "name": "esp-best-skeleton",
    "description": "ESP Best Practices Angular Skeleton",
    "version": "5.3.0",
    "keywords": [
        "esp"
    ],
    "dependencies": {
        "esp-angular-skeleton": "~5.3",
        "less": ">=1.3.3",
        "bootstrap": "~3",
        "angular-bootstrap": ">=0.7.0",
        "html5shiv": "*",
        "font-awesome": "*",
        "more": "*",
        "respond": "*"
    },
    "licenses": [ {
            "type": "GPL",
            "url": "http://www.gnu.org/licenses/gpl-2.0.html"
        }, {
            "type": "Commercial",
            "url": "https://embedthis.com/licensing/"
        }
    ],
    "export": [
        {  "from": "js/*.js",   "to": "esp-best-skeleton/", "type": "js"    },
        {  
            "from": "documents/**", 
            "to": "${TOP}", 
            "overwrite": false
        }
    ],
    "blend": [
        "esp.json",
        "expansive.json"
    ],
    "skeleton": true
}
