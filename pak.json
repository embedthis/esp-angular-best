{
    "name": "esp-angular-best",
    "description": "ESP Best Practices Angular",
    "version": "6.0.0",
    "warn": "This pak requires Angular 1 which is no longer supported.",
    "keywords": [
        "esp"
    ],
    "licenses": [
        {
            "type": "GPL",
            "url": "http://www.gnu.org/licenses/gpl-2.0.html"
        },
        {
            "type": "Commercial",
            "url": "https://embedthis.com/licensing/"
        }
    ],
    "repository": {
        "type": "git",
        "url": "git://github.com/embedthis/esp-angular-best.git"
    },
    "dependencies": {
        "esp-angular-skeleton": "~6.0",
        "html5shiv": "~3.7",
        "font-awesome": "~4.3",
        "more": "~0.0",
        "respond": "~1.4"
    },
    "devDependencies": {
        "pak": ">=0.11.3"
    },
    "export": [
        {
            "from": "js/*.js",
            "to": "esp-angular-best/"
        },
        {
            "from": "contents/**",
            "to": "${TOP}",
            "overwrite": false
        }
    ],
    "manage": "esp.json"
}
